      ELSEIF ( ITYPE .GT. 100 ) THEN				      ELSEIF ( ITYPE .GT. 100 ) THEN
        IF (DEBUG) WRITE(MDEBUG,*) 'NUCINT: HEAVY PRIMARY EKI	        IF (DEBUG) WRITE(MDEBUG,*) 'NUCINT: HEAVY PRIMARY EKI
     *                                               SNGL(EKI	     *                                               SNGL(EKI
